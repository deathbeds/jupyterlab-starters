name: jupyterlab-starters

channels:
  - conda-forge
  - defaults

dependencies:
  - cookiecutter
  - firefox =78
  - geckodriver
  - jupyterlab {lab}
  - nodejs >=14,<15
  - pytest-asyncio
  - pytest-azurepipelines
  - pytest-cov
  - pytest-flake8
  - python {python}
  - robotframework >=3.2
  - robotframework-seleniumlibrary
